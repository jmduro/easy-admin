{
  "name": "easy-admin",
  "displayName": "Easy Admin",
  "description": "Organizaci√≥n de tareas eficientes en entornos de desarrollo",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.83.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "easy-admin.helloWorld",
        "title": "Hello World",
        "category": "Easy-Admin"
      },
      {
        "command": "easy-admin.tareas",
        "title": "Easy-Admin Tareas",
        "category": "Easy-Admin"
      },
      {
        "command": "easy-admin.colaboradores",
        "title": "Easy-Admin Colaboradores",
        "category": "Easy-Admin"
      },
      {
        "command": "easy-admin.calendario",
        "title": "Easy-Admin Calendario",
        "category": "Easy-Admin"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "easy-admin",
          "title": "Easy Admin",
          "icon": "resources/icon.svg"
        }
      ]
    },
    "views": {
      "easy-admin": [
        {
          "id": "tasks",
          "name": "Tareas"
        },
        {
          "id": "helpers",
          "name": "Colaboradores"
        },
        {
          "id": "calendar",
          "name": "Calendario"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.83.0",
    "@types/mocha": "^10.0.3",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "^8.52.0",
    "glob": "^10.3.10",
    "mocha": "^10.2.0",
    "typescript": "^5.2.2",
    "@vscode/test-electron": "^2.3.6"
  }
}